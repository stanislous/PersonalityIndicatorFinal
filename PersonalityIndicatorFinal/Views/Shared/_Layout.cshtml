<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Myers-Briggs Personality Indicator</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="https://static.neris-assets.com/images/favicons/mstile-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <link rel="stylesheet" href="https://static.neris-assets.com/css/main-core.css?id=054664c920607d9f0dc3">
    <link rel="stylesheet" href="https://www.16personalities.com/css/fonts.css">
    <link rel="stylesheet" href="https://www.16personalities.com/css/font-awesome.min.css"> 
    <link rel="stylesheet" href="https://static.neris-assets.com/css/main-core.css?id=054664c920607d9f0dc3">
    <link rel="stylesheet" href="https://www.16personalities.com/css/fonts.css">
    <link rel="stylesheet" href="https://static.neris-assets.com/css/main-noncore.css?id=b84d75099d5b26632509">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,600|Open+Sans:400,400i,600,600i,700&subset=cyrillic,latin-ext" rel="stylesheet">
    <link rel="stylesheet" href="https://www.16personalities.com/css/font-awesome.min.css">
    <style>
        [v-cloak] {
            display: none !important;
        }
        .navbar-inverse .navbar-brand {
            color: #ffffff;
        }
        .navbar-inverse .navbar-nav > li > a {
            color: #ffffff;
        }
    </style>
</head>
<body style="padding-top: 0px">
    @Html.Partial("_NavBar")
    <div class="container-fluid" style="padding-left: 0; padding-right: 0;">
        @RenderBody()
        <hr/>
        <footer>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
